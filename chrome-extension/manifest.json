{
	"name": "Jupiterlab hot reload",
	"version": "1.0",
	"manifest_version": 3,
	"description": "Reloads the jupiterlab file when using jupiter notebook share",
	"icons": {
		"16": "icon-16.png",
		"48": "icon-48.png",
		"128": "icon-128.png"
	},
	"content_scripts": [
		{
			"matches": ["http://localhost:5500/*", "http://localhost:8888/*", "https://kassu11.github.io/jupiter-notebook-sharing/*"],
			"js": ["content.js"],
			"all_frames": true,
			"run_at": "document_end"
		}
	],
	"permissions": ["storage"]
}
