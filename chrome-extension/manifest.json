{
	"name": "Jupyterlab hot reload",
	"version": "1.0",
	"manifest_version": 3,
	"description": "Reloads the jupyterlab file when using jupiter notebook share",
	"icons": {
		"16": "icon-16.png",
		"48": "icon-48.png",
		"128": "icon-128.png"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"],
			"all_frames": true,
			"run_at": "document_end"
		}
	],
	"permissions": ["storage"]
}
